/* IE10 Consumer Preview */ 
background-image: -ms-linear-gradient(bottom, #D8D4D4 0%, #FFFFFF 84%, #D8D4D4 100%) repeat scroll 0 0 transparent;

/* Mozilla Firefox */ 
background: -moz-linear-gradient(center bottom , #D8D4D4 0%, #FFFFFF 84%, #D8D4D4 100%) repeat scroll 0 0 transparent;

/* Opera */ 
background-image: -o-linear-gradient(bottom, #D8D4D4 0%, #FFFFFF 84%, #D8D4D4 100%);

/* Webkit (Safari/Chrome 10) */ 

	background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, #E6E6E6), color-stop(0.8, white), color-stop(1, #EBEBEB));

/* Webkit (Chrome 11+) */ 

	background-image: -webkit-linear-gradient(bottom, #F1F1F1 0%, white 84%, #EBEBEB 100%) repeat scroll 0 0 transparent;

/* W3C Markup, IE10 Release Preview */ 

	background-image: linear-gradient(to top, #ECECEC 0%, white 84%, #E4E4E4 100%) repeat scroll 0 0 transparent;
	
	overflow-x:hidden;
}

